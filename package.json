{
  "name": "@jupiterone/jupiterone-mcp",
  "version": "0.0.11",
  "description": "Model Context Protocol server for JupiterOne account rules and rule details",
  "main": "dist/index.js",
  "bin": {
    "jupiterone-mcp": "dist/index.js"
  },
  "scripts": {
    "prebuild": "npx tsx scripts/generate-descriptions.ts",
    "build": "npm run prebuild && tsc",
    "dev": "npm run build && npx nodemon --watch src --ext ts --ignore src/generated --exec 'npm run build'",
    "inspector": "npx dotenv -e .env -- npx @modelcontextprotocol/inspector node dist/index.js",
    "start": "node dist/index.js",
    "test": "jest",
    "test-connection": "node test-connection.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "jupiterone",
    "security",
    "rules",
    "alerts",
    "graphql"
  ],
  "author": "JupiterOne MCP Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.0",
    "dotenv": "^16.5.0",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}
